{
  "name": "Golha",
  "description": "Electron + Create React App + Electron Builder",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Saeed Heidari",
    "email": "saeed.heidari86@gmail.com",
    "url": "http://boum.ir"
  },
  "build": {
    "appId": "ir.boum.golha-electron",
    "productName": "Radio Golha Desktop App",
    "copyright": "Copyright Â© 2020 boum.ir",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "electron-is-dev": "^1.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "node-sass": "^4.13.1",
    "pouchdb-adapter-idb": "^7.2.1",
    "rc-slider": "^9.2.4",
    "react": "^16.13.1",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.13.1",
    "react-lazy-load-image-component": "^1.4.1",
    "react-lottie": "^1.2.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "rxdb": "^8.8.0",
    "rxjs": "^6.5.4"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "eslint . && concurrently 'BROWSER=none yarn start' 'wait-on http://localhost:3000 && electron .'",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "./node_modules/.bin/electron-builder build -mw",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "commitlint": "^8.3.5",
    "concurrently": "^5.1.0",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "prettier": "2.0.2",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  }
}
